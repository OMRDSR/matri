	<script>
	var queryString = window.location.search, urlParams = new URLSearchParams(queryString);
	if(isMobile()){
		if(!urlParams.get('first')){
			document.getElementById('dt-header').classList.remove("bgCorp");
			document.getElementById('dt-header').classList.add("bgSecond");
			document.getElementById('dh-header').classList.remove("oculto");
			document.getElementById('navigation').classList.add("oculto");
			document.getElementsByClassName('ulNavigation')[0].setAttribute('style','bottom:70px');
			document.getElementById('contenido').setAttribute('style','width:100%');
		}else{
			document.getElementsByClassName('ulNavigation')[0].setAttribute('style','bottom:70px');
			document.getElementById('contenido').setAttribute('style','width:100%');
			mostrarBotonMenu();
		}
	}else{
		document.getElementById('navigation').setAttribute('style','width:220px');
		document.getElementsByClassName('ulNavigation')[0].setAttribute('style','top:150px');
		document.getElementById('contenido').setAttribute('style','width: calc(100% - 220px)');
	}
	validarLogoMenu();
	function isMobile(){
	  return (
		(navigator.userAgent.match(/Android/i)) ||
		(navigator.userAgent.match(/webOS/i)) ||
		(navigator.userAgent.match(/iPhone/i)) ||
		(navigator.userAgent.match(/iPod/i)) ||
		(navigator.userAgent.match(/iPad/i)) ||
		(navigator.userAgent.match(/BlackBerry/i))
	  );
	}
	function mostrarBotonMenu(){
		document.getElementsByClassName('principal')[0].removeAttribute('style');
		document.getElementById('navigation').setAttribute('style','width:100%');
		document.getElementById('navigation').classList.remove("oculto");
		document.getElementById('contenido').classList.add("oculto");
		validarLogoMenu();
	}
	function loadRedimencionar(){
		if(widthSize() < 780){
			if(!urlParams.get('first')){
				document.getElementsByClassName('principal')[0].setAttribute('style','width:800px');
			}
		}
	}
	</script>
